pipeline {
    agent any

    stages {

        stage('Infos Git') {
            steps {
                bat 'git --version'
            }
        }

        stage('Nombre de commits') {
            steps {
                bat 'git rev-list --count HEAD'
            }
        }

        stage('Liste des commits') {
            steps {
                bat 'git log --oneline -5'
            }
        }

    }
}
